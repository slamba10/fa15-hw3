<h1> WELCOME </h1>
<h2> Cats </h1>
	
		<% @cat.each do |element|%>
			<p><%= element.name%></p>
		<% end %>
	
<h2> TODOs </h2>
	
		<% @todo.each do |element|%>
			<p><%= element.tasks%> <%= element.finished%></p>
		<% end %>
	
<h2> Users </h2>
		<% @user.each do |element|%>
			<p><%= element.name%><%=element.age%></p>
		<% end %>

